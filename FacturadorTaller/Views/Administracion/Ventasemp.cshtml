@model FacturadorTaller.ViewModel.DetalleCotViewModel
@{
    ViewBag.Title = "Ventasemp";
}

<h2>Trabajos Facturados</h2>

<table class="table">
    <tr>
        <th>
           Fecha
        </th>
        <th>
            Cantidad
        </th>
        <th>
            Producto
        </th>
        <th>
            Precio
        </th>
        <th>
            Cotizacion
        </th>
    </tr>

    @foreach (var item in Model.DetalleCot)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelitem => item.Cotizacion.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelitem => item.Cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelitem => item.Producto.NombreProducto)
        </td>
        <td>
            @Html.DisplayFor(modelitem => item.Valor)
        </td>
        <td>
            @Html.DisplayFor(modelitem => item.CotizacionId)
        </td>
        <td>
            @Html.DisplayFor(modelitem => item.Cotizacion.Clientes.NombreCliente)
        </td>
    </tr>
    }
</table>

<div>
    @Html.ActionLink("Volver a lista", "Index")

</div>
